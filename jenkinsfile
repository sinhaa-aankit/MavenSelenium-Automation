pipeline {
	agent any
	tools{
		maven 'Maven3.8.3'
	}
	stages{
		stage('Automation_101') {
			steps {
				echo 'Starting'
			}
		}
		stage('Build') {
			steps{
				git branch: 'main', url: 'https://github.com/sinhaa-aankit/MavenSelenium-Automation/blob/main/src/main/java/automationTesting/automation/App.java'
				sh 'mvn clean compile'
			}
		}
	}
}